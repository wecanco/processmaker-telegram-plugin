{
  "name": "wecanco/processmaker-telegram-plugin",
  "description": "ProcessMaker plugin for Telegram notifications with inline actions",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "WeCanCo",
      "email": "info@wecan-co.ir"
    }
  ],
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi || true"
    ]
  },
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "ProcessMaker\\TelegramPlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ProcessMaker\\TelegramPlugin\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ProcessMaker\\TelegramPlugin\\Providers\\PluginServiceProvider"
      ],
      "dont-discover": []
    },
    "processmaker": {
      "plugin-type": "standalone",
      "name": "Telegram Notifications",
      "description": "Send task notifications to Telegram with inline actions",
      "version": "1.0.0",
      "compatibility": {
        "processmaker": ">=4.0"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}